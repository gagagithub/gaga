      <% if session[:webusername]!=nil %>
          <a class="pull-right rounder" onclick="logout()" href="javascript:void(0)">注销</a><br>
      <a class="pull-right rounder"><%=session[:webusername]%></a>
      <% else %>
          <a class="pull-right rounder" href="/" id="layout_login">登陆</a>
          <a class="pull-right rounder" href="/user/signup" id="layout_login">注册</a>
      <% end %>

